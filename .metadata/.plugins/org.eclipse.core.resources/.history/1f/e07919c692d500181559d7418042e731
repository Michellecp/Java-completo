package entities;

public class Empregados_list {
	public int Id;
	public String name;
	public double salario;
	
	public Empregados_list(int id, String name, double salario) {
		this.Id = id;
		this.name = name;
		this.salario = salario;
	}

	public int getId() {
		return Id;
	}

	public void setId(int id) {
		Id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getSalario() {
		return salario;
	}

	public void setSalario(double salario) {
		this.salario = salario;
	}
	
	
	
	

}
