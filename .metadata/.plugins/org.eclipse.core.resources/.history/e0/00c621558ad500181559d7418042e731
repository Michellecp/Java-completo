package application;

import java.util.List;
import java.util.ArrayList;

public class Program5 {

	public static void main(String[] args) {
		
		List<String> list = new ArrayList<>();
		
		list.add("Maria");
		list.add("Alex");
		list.add("Bob");
		list.add("Anna");
		list.add(2, "Marco");
		
		System.out.println(list.size());
		
		list.remove("Anna");

		for(String x : list){
			System.out.println(x);
		}
	}

}
