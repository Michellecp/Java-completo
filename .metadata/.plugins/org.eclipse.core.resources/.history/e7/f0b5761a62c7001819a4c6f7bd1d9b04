package course;

import java.util.Scanner;

public class Program6 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int n1 =sc.nextInt();
		sc.nextLine();
		String name =  sc.nextLine();
		
		System.out.println(n1);
		System.out.print(name);
		
		
		
		
		
		
		sc.close();

	}

}
